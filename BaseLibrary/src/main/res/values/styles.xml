<resources>


    <!--默认样式 注意* 样式只可修改，不能删除任何一项 否则报错-->
    <style name="picture.style" parent="picture.default.style">
        <!-- Customize your theme here. -->
        <!--标题栏背景色-->
        <item name="colorPrimary">@color/color_white</item>
        <!--状态栏背景色-->
        <item name="colorPrimaryDark">@color/color_white</item>
        <!--是否改变图片列表界面状态栏字体颜色为黑色-->
        <item name="picture.statusFontColor">true</item>
        <!--返回键图标-->
        <item name="picture.leftBack.icon">@mipmap/icon_back_36</item>
        <!--标题下拉箭头-->
        <item name="picture.arrow_down.icon">@mipmap/icon_arrow_down_black_14</item>
        <!--标题上拉箭头-->
        <item name="picture.arrow_up.icon">@mipmap/icon_arrow_up_black_14</item>
        <!--标题文字颜色-->
        <item name="picture.title.textColor">@color/color_title_dark</item>
        <!--标题栏右边文字-->
        <item name="picture.right.textColor">@color/color_title_dark</item>
        <!--图片列表勾选样式-->
        <item name="picture.checked.style">@drawable/selector_check_bg</item>
        <!--开启图片列表勾选数字模式-->
        <item name="picture.style.checkNumMode">false</item>
        <!--选择图片样式0/9-->
        <item name="picture.style.numComplete">false</item>
        <!--图片列表底部背景色-->
        <item name="picture.bottom.bg">@color/color_fa</item>
        <!--图片列表预览文字颜色-->
        <item name="picture.preview.textColor">@color/color_title_dark</item>
        <!--图片列表已完成文字颜色-->
        <item name="picture.complete.textColor">@color/tab_color_true</item>
        <!--图片已选数量圆点背景色-->
        <item name="picture.num.style">@drawable/num_oval</item>
        <!--预览界面标题文字颜色-->
        <item name="picture.ac_preview.title.textColor">@color/white</item>
        <!--预览界面已完成文字颜色-->
        <item name="picture.ac_preview.complete.textColor">@color/tab_color_true</item>
        <!--预览界面标题栏背景色-->
        <item name="picture.ac_preview.title.bg">@color/color_title_dark</item>
        <!--预览界面底部背景色-->
        <item name="picture.ac_preview.bottom.bg">@color/bar_grey_90</item>
        <!--预览界面返回箭头-->
        <item name="picture.preview.leftBack.icon">@mipmap/icon_back_w_36</item>

    </style>


</resources>
