<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:clickable="true"
    android:layout_height="wrap_content"
   >

    <com.sang.common.widget.CircularFramlayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/app_gap"
        android:layout_marginRight="@dimen/app_gap"
        android:layout_marginTop="@dimen/app_model_gap"
        android:background="@color/color_white"
        android:padding="@dimen/app_gap"
        app:viewradius="4dp"
        >
        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_car_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/color_title_dark"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_cat_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="9dp"
                android:background="@drawable/shap_2_tv_bg"
                android:paddingLeft="2dp"
                android:paddingRight="2dp"
                android:text="TextView"
                android:textColor="@color/color_white"
                android:textSize="11sp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_car_num"
                app:layout_constraintStart_toEndOf="@+id/tv_car_num"
                app:layout_constraintTop_toTopOf="@+id/tv_car_num" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/app_item_gap"
                android:text="车主："
                android:textColor="@color/color_order_color"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/tv_car_num"
                app:layout_constraintTop_toBottomOf="@+id/tv_car_num" />

            <TextView
                android:id="@+id/tv_owner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_order_color"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="@+id/textView4"
                app:layout_constraintStart_toEndOf="@+id/textView4"
                app:layout_constraintTop_toTopOf="@+id/textView4" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="@dimen/img_go_size"
                android:layout_height="@dimen/img_go_size"
                android:src="@mipmap/icon_gary_arrowmore_20"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </android.support.constraint.ConstraintLayout>

    </com.sang.common.widget.CircularFramlayout>

</FrameLayout>